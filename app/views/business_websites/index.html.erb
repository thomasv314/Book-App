<h1>Listing business_websites</h1>

<table>
  <tr>
    <th>Domain name</th>
    <th>Domain suffix</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @business_websites.each do |business_website| %>
  <tr>
    <td><%= business_website.domain_name %></td>
    <td><%= business_website.domain_suffix %></td>
    <td><%= link_to 'Show', business_website %></td>
